{
  "name": "yoavshop",
  "version": "1.0.0",
  "description": "SecondLive Shop build with MERN stack",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder.js",
    "data:destroy": "node backend/seeder.js -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YoavDdev/yoavshop.git"
  },
  "author": "Yoav Drai",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/YoavDdev/yoavshop/issues"
  },
  "homepage": "https://github.com/YoavDdev/yoavshop#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "express": "^4.18.2",
    "mongoose": "^7.2.2"
  },
  "devDependencies": {
    "concurrently": "^8.1.0",
    "dotenv": "^16.1.3",
    "nodemon": "^1.14.9"
  }
}
